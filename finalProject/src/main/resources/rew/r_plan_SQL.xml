<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="R_Plan">
	
	<select id="selectAllById" parameterType="R_PlanDTO" resultType="R_PlanDTO">
	SELECT * FROM PLAN 
	WHERE MID=#{mid} and PTITLE=#{pid}
	order by NUM;
	</select>
	
</mapper>

