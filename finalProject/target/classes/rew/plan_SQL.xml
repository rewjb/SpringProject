<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="PlaceCart">
	<insert id="insert" parameterType="PlaceCartDTO">
	insert into PLACECART values(#{mid},#{pid})
	</insert>
	
	<select id="selectAllById" parameterType="String" resultType="PlaceCartDTO">
	select * from PLAN
	where MID='temp'
	order by NUM
	</select><!-- PID/TITLE/LATITUDE/LONGITUDE/MAINIMG -->
	
</mapper>

